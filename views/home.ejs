<%- include('partials/header'); %> 

<h1>Home</h1>
<% items.forEach(function(post){ %>
    <div class="blog">
        <h1><%= post.title %> </h1>
        <p>
            <%= post.content.substring(0,100)+ "...." %> 
            <a href="/posts/<%= post.title %> ">Read More</a>
        </p>
    </div>
<% }); %>

<%- include('partials/footer'); %> 